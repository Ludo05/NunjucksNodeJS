{% macro cardSwatch(colorName, colorValue, colorNotes) %}
<div class="color-swatch-tile">

    <div class="color-swatch"
         style="background-color: {{ colorValue }};">
    </div>

    <div class="color-name">
        <p> {{ colorName }}</p>
    </div>

    <div class="color-notes">
        {{ colorNotes }}
    </div>

</div>
{% extends "index.html" %}

{% endmacro %}
